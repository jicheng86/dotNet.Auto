<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>@ViewBag.Title - dotNet.Auto</title>
    <environment exclude="Development">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css"
              integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
              crossorigin="anonymous"
              asp-fallback-test-class="sr-only"
              asp-fallback-src="~/lib/bootstrap/v4.5.0/dist/css/bootstrap.min.css"
              type="text/css"
              asp-append-version="true" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css"
              asp-fallback-src="~/lib/fontawesome/v5.11.2/dist/css/all.css"
              integrity="sha512-0S+nbAYis87iX26mmj/+fWt1MmaKCv80H+Mbo+Ne7ES4I6rxswpfnC6PxmLiw33Ywj2ghbtTw0FkLbMWqh4F7Q=="
              crossorigin="anonymous"
              type="text/css"
              asp-append-version="true" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/animate.css@4.1.0/animate.min.css"
              asp-fallback-href="~/lib/animate/v4.1.0/dist/animate.css"
              integrity="sha256-6hqHMqXTVEds1R8HgKisLm3l/doneQs+rS1a5NLmwwo="
              crossorigin="anonymous"
              type="text/css"
              asp-append-version="true">
    </environment>
    <environment include="Development">
        <link href="~/lib/bootstrap/v4.5.0/dist/css/bootstrap.css" rel="stylesheet" type="text/css" asp-append-version="true" />
        <link href="~/lib/fontawesome/v5.11.2/dist/css/all.css" rel="stylesheet" type="text/css" asp-append-version="true" />
        <link href="~/lib/animate/v4.1.0/dist/animate.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    </environment>
    <link href="~/css/inspina.css" rel="stylesheet" />
    <link href="~/css/public.css" rel="stylesheet" type="text/css" />

    <!-- Add local styles, mostly for plugins css file -->
    @RenderSection("Styles", required: false)
</head>
<body class="container-fluid pace-done">

    <!-- Wrapper-->
    <div id="wrapper" class="@Html.PageClass() ">

        <!-- Navigation -->
        <partial name="_Navigation" />

        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg @ViewBag.SpecialClass">

            <!-- Top Navbar -->
            <partial name="_TopNavbar" />

            <!-- Main view  -->
            @RenderBody()

            <!-- Footer -->
            <partial name="_Footer" />

        </div>
        <!-- End page wrapper-->
        <!-- Right Sidebar -->
        <partial name="_RightSidebar" />

    </div>
    <!-- End wrapper-->
    @RenderSection("toast", required: false)
    @*jquery-v3.5.1 metisMenu slimscroll popper *@
    <environment exclude="Development">
        <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                asp-fallback-src="~/lib/jquery/v3.5.1/dist/jquery.min.js"
                integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
                crossorigin="anonymous"
                type="text/javascript"
                asp-append-version="true"></script>
        <script src="https://cdn.jsdelivr.net/npm/metismenu@2.7.7/dist/metisMenu.min.js"
                asp-fallback-src="~/lib/jquery-metisMenu/v2.7.7/dist/metisMenu.min.js"
                integrity="sha256-dsY+tXuUzKKev+KTbuLwRITlSxSeAjfXMZhNV31/dHQ="
                crossorigin="anonymous" asp-append-version="true"></script>
        <script src="https://cdn.jsdelivr.net/npm/jquery-slimscroll@1.3.8/jquery.slimscroll.min.js"
                asp-fallback-src="~/lib/jquery-slimScroll/v1.3.8/dist/jquery.slimscroll.min.js"
                integrity="sha256-qE/6vdSYzQu9lgosKxhFplETvWvqAAlmAuR+yPh/0SI="
                crossorigin="anonymous"
                type="text/javascript"
                asp-append-version="true"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.13.0/umd/popper.min.js"
                asp-fallback-src="~/lib/popper/v1.13.0/dist/umd/popper.js"
                integrity="sha512-Ua/7Woz9L5O0cwB/aYexmgoaD7lw3dWe9FvXejVdgqu71gRog3oJgjSWQR55fwWx+WKuk8cl7UwA1RS6QCadFA=="
                crossorigin="anonymous"
                type="text/javascript"
                asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/lib/jquery/v3.5.1/dist/jquery.min.js" type="text/javascript" asp-append-version="true"></script>
        <script src="~/lib/jquery-metisMenu/v2.7.7/dist/metisMenu.min.js" type="text/javascript" asp-append-version="true"></script>
        <script src="~/lib/jquery-slimScroll/v1.3.8/dist/jquery.slimscroll.min.js" type="text/javascript" asp-append-version="true"></script>
        <script src="~/lib/popper/v1.13.0/dist/umd/popper.min.js"></script>
    </environment>
    @* bootstrap-v4.5.0 bundle自包含popper 依赖jquery *@
    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.bundle.js"
                integrity="sha512-iqhWkvLOXVDz+Lr//ZryEKNvZ5pmgdKEe58Wh/VwfTGwTku0MKbuLhjJ1zUAJu8iSbOqfLcXrrxJ61+27THi2Q=="
                crossorigin="anonymous"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                asp-fallback-src="~/lib/bootstrap/v4.5.0/dist/js/bootstrap.bundle.min.js"
                type="text/javascript"
                asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/lib/bootstrap/v4.5.0/dist/js/bootstrap.bundle.js" asp-append-version="true"></script>
    </environment>
    <script src="~/js/inspina.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/lib/layer/v3.11/dist/layer.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/js/public.js" type="text/javascript" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
